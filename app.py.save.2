from flask import Flask, jsonify
from oct2py import octave

app = Flask(__name__)

@app.route('/table', methods=['GET'])
def benchop():
    octave.addpath('/home/ubuntu/BENCHOP')
    octave.run('Table.m')

    return 'ok'


if __name__ == '__main__':
    app.run(host='0.0.0.0',debug=True)

